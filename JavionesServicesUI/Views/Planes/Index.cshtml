@{
}

<h3 class="my-5">Listado de aviones, ordenados según ocupación</h3>

<table class="table">
    <tr>
        <th>Matricula</th>
        <th>Marca</th>
        <th>Modelo</th>
        <th>Revisiones</th>
        <th>Año de fabricación</th>
    </tr>

    @foreach (var plane in Model)
    {
        <tr>
            <td>@plane.LicensePlate</td>
            <td>@plane.Brand</td>
            <td>@plane.Model</td>
            <td>@plane.Revision</td>
            <td>@plane.FabricationYear</td>
        </tr>
    }
</table>