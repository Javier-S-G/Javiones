@{
}
<h3>Listado de aviones aerolinea @Model.AirlineName</h3>
<ul>
    @foreach (var plane in Model.AirlinePlanes)
    {
        <li>Avión @plane.LicensePlate, Marca: @plane.Brand, Modelo: @plane.Model.Length Fabricado el año @plane.FabricationYear </li>
    }
</ul>
<button class="btn btn-primary" onclick="location.href='@Url.Action("Index", "Airline")'">Volver</button>